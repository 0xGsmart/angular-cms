{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "es2015",
    "types": [],
    "paths": {
      "@angular/*": [
        "../node_modules/@angular/*"
      ],
      "rxjs/*": [
        "../node_modules/rxjs/*"
      ]
    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ],
  "include": [
    "./",
    "../../cms/core/index.ts",
    "../../cms/editor/index.ts",
    "../../cms/form-elements/index.ts",
    "../node_modules/@angular-cms/portal/src/cms.module.ts"
  ]
}