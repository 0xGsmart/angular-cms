{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist",
    "sourceMap": true,
    "declaration": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "lib": [
      "es2016",
      "dom"
    ],
    "paths": {
      "@angular/*": [
        "../../cms-examples/node_modules/@angular/*"
      ],
      "rxjs/*": [
        "../../cms-examples/node_modules/rxjs/*"
      ]
    }
  },
  "include": [
    "src"
  ]
}
